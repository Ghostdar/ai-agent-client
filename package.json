{
  "name": "ai-agent-client",
  "version": "0.0.1",
  "description": "created with create-my-express",
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:mcp": "tsc -p tsconfig.mcp.json",
    "build:all": "npm run build && npm run build:mcp",
    "copy:mcp": "mkdir -p dist/mcp-package && cp -r mcp-package/*.js dist/mcp-package/ 2>/dev/null || true",
    "start": "cross-env NODE_ENV=production dotenv -e .env node dist/src/index.js",
    "start:mcp": "node dist/mcp-package/google-search.js",
    "dev": "cross-env NODE_ENV=development nodemon --config nodemon.json",
    "dev:mcp": "ts-node --esm -P tsconfig.mcp.json mcp-package/google-search.ts",
    "test": "cross-env NODE_ENV=test dotenv -e .env jest",
    "test:watch": "cross-env NODE_ENV=test dotenv -e .env jest --watch",
    "lint": "npx eslint src",
    "mcp:start": "node dist/mcp-package/google-search.js",
    "mcp:dev": "ts-node --esm -P tsconfig.mcp.json mcp-package/google-search.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@modelcontextprotocol/sdk": "^1.8.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.4",
    "cheerio": "^1.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.5.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
